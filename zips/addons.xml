<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.tvprowizard" name="[COLOR blue]TvProTeam Wizard[/COLOR]" version="3.0.1" provider-name="[COLOR blue]TvProTeam Wizard[/COLOR]">
    <requires>
        <import addon="xbmc.python" version="3.0.1"/>
        <import addon="script.module.requests"/>
        <import addon="script.module.six"/>
    </requires>

    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
    </extension>

    <extension point="xbmc.service" library="startup.py" start="startup" />

    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">[COLOR blue]TvProTeam Wizard[/COLOR]</summary>
        <description lang="en_GB">[COLOR blue]TvProTeam Wizard para instalar, actualizar y realizar mantenimiento al build Latinokodi[/COLOR]</description>
        <disclaimer lang="en_GB">The author of this addon does not host any content and has no affiliation with the content providers</disclaimer>
        <platform>all</platform>
        <reuselanguageinvoker>false</reuselanguageinvoker>
        <source></source>
        <news></news>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
		</assets>
		<license>GNU GENERAL PUBLIC LICENSE Version 3</license>
		<email></email>
		<website></website>
		<forum>https://t.me/hackerteam</forum>
    </extension>
</addon>

<addon id="repository.TvProTeam" name="TvProTeam add-on repository" version="1.0.9" provider-name="TvPro">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://tvproteam.github.io/zips/addons.xml</info>
            <checksum>https://tvproteam.github.io/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://tvproteam.github.io/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>TvProTeam Repository</summary>
        <description>TvProTeam Repository</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
