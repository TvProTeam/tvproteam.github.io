<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="repository.lku" name="Repositorio LKU" version="1.0.0" provider-name="[COLOR purple]•LKU•[/COLOR]">
    <extension point="xbmc.addon.repository" name="Repositorio LKU">
        <dir minversion="18.9.0">
            <info compressed="false">https://latinokodi.lat/addons/addons.xml</info>
            <checksum>https://latinokodi.lat/addons/addons.xml.md5</checksum>
            <datadir zip="true">https://latinokodi.lat/addons/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>Repositorio LKU</summary>
        <description>Repositorio LKU</description>
        <platform>all</platform>
    <assets>
        <icon>icon.png</icon>
        <fanart>fanart.jpg</fanart>
    </assets>
    </extension>
</addon>

<addon id="plugin.program.hackerwizard" name="HackerWizard Team" version="1.1" provider-name="Hacker by Cristain">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Hacker WIzard Team</summary>
    <description lang="en">Wizard para HackerTeam by Cristian</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.TvProTeam" name="TvProTeam add-on repository" version="1.0.9" provider-name="TvPro">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://tvproteam.github.io/zips/addons.xml</info>
            <checksum>https://tvproteam.github.io/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://tvproteam.github.io/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>TvProTeam Repository</summary>
        <description>TvProTeam Repository</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
